!---------------------------------------------------------------!
! Type: URxvt.Config						!
! 								!
!---------------------------------------------------------------!

*.font: xft:monofur for Powerline:style=Regular:size=13
*.boldFont: xft:monofur for Powerline:style=Bold:size=13
*.italicFont: xft:monofur for Powerline:style=Italic:size=13
!*.boldItalicFont: xft:Iosevka:style=Medium Italic:size=11
!
rofi.font: InconsolataForPowerline Nerd Font 11
URxvt.scrollBar: false



!---------------------------------------------------------------!
! Type: URxvt.Extensions					!
! 								!
!---------------------------------------------------------------!

urxvt.perl-ext-common: resize-font, clipboard


!---------------------------------------------------------------!
! Type: URxvt.Keybindings					!
! 								!
!---------------------------------------------------------------!

URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset

!URxvt.keysym.Shift-Meta-V: eval:paste_clipboard
!URxvt.keysym.Shift-Meta-C: eval:selection_to_clipboard
!URxvt.keysym.M-c: perl:clipboard:copy
!URxvt.keysym.M-v: perl:clipboard:paste



!---------------------------------------------------------------!
! Type: URxvt.DisableKeybindings				!	!
! 								!
!---------------------------------------------------------------!

!URxvt.keysym.Control-Meta-c: builtin-string:
!URxvt.keysym.Control-Meta-v: builtin-string:



